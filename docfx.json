{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Celeste.Mod.mm/bin/Debug/**.mm.dll"
          ],
          "src": "../Everest/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "tutorials/**"          
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "index.html",
          "main/**",
          "images/**",
          "fonts/**",
          "downloads/**",
          "logo*",
          "favicon*",
          "modupdater.txt"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": ["global.json"],
    "fileMetadataFiles": [],
    "template": [
      "template_everest"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}